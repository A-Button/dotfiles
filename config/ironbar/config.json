{
  "position": "top",
  "height": 34,
  "margin.top": 0,
  "margin.left": 0,
  "margin.right": 0,
  "start": [
    {
      "type": "workspaces",
      "favorites": ["1", "2"],
      "all_monitors": false
    }
  ],
  "center": [
    {
      "type": "music",
      "player_type": "mpris",
      "format": "{title} / {artist}",
      "truncate": "end",
      "icons": {
        "play": "",
        "pause": ""
      }
    }
  ],
  "end": [
    {
      "type": "notifications",
      "show_count": true,
      "icons": {
        "closed_none": "󰍥",
        "closed_some": "󱥂",
        "closed_dnd": "󱅯",
        "open_none": "󰍡",
        "open_some": "󱥁",
        "open_dnd": "󱅮"
      }
    },
    {
      "format": [
        " {cpu_percent}% | {cpu_frequency} GHz",
        " {memory_used} / {memory_total} GB"
      ],
      "interval": {
        "cpu": 1,
        "disks": 300,
        "memory": 30,
        "networks": 3,
        "temps": 5
      },
      "type": "sys_info"
    },
    {
      "type": "volume",
      "format": "{icon} {percentage}%",
      "max_volume": 100,
      "truncate": "middle",
      "icons": {
        "volume_high": "󰕾",
        "volume_medium": "󰖀",
        "volume_low": "󰕿",
        "muted": "󰝟"
      }
    },
    {
      "type": "tray"
    },
    {
      "type": "clock",
      "format": "%Y/%m/%d %H:%M"
    }
  ]
}
